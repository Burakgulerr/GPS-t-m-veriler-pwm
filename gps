/* USER CODE BEGIN PD */
typedef struct
{
	char section[280];

	char sentenceIdentifier[7];		//1
	float time;						//2
	double latitude;				//3
	char latitudeDir;				//4
	double longtitude;				//5
	char longtitudeDir;				//6
	char fix_quality;				//7
	int numberOfSatellites;			//8
	float horizontal_dilution;		//9
	double altitude;				//10
}GNGGA;


//GNGGA gps_parser(char* rawData, int len);

char raw_array[300];

char anahtar[300] = "$GNGGA";
int i;
int j;
int counter;
int virgul_sayisi = 0;
int x;
GNGGA DATALAR;

/* USER CODE END PD */

